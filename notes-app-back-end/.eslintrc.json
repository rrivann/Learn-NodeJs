{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "object-curly-spacing": "off",
        "no-unused-vars": "off",
        "comma-dangle": "off"
    }
}
